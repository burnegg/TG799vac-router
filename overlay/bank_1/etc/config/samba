config samba
        option 'name'                   'Telstra-Gateway'
        option 'workgroup'              'WORKGROUP'
        option 'description'            'Telstra Gateway'
        option 'homes'                  '1'
        option 'enabled'                '1'
        option 'charset'                'ISO8859-1'

config 'sambashare'
        option 'name' 'usbdisk'
        option 'path' '/mnt/usb'
        option 'guest_ok' 'yes'
        option 'create_mask' '0700'
        option 'dir_mask' '0700'
        option 'read_only' 'no'
        option 'enabled' '1'

